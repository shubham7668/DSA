class Solution {                       //MUMMY MEREKO NAHI KARNA DSA ðŸ˜­ðŸ˜­ðŸ˜­ðŸ˜­
public:
    string reverseWords(string s) {
       
        vector<string> str;
        int i=0,flag=0,n=s.length(); 
        string word="";
        
        for(i=0;i<n;i++){           //For trailing spaces
            if(s[i]!=' ') break;
        }
        
        for(int j=i;j<n;j++){
            if(s[j]!=' '){         //For storing 1st word of s
                word+=s[j];
                flag=1;
            }
            else if(s[j]==' ' && flag==1){  //Push that word in str 
                str.push_back(word);
                word="";
                flag=0;
            }
        }
        
        if(word.length()!=0) str.push_back(word);
        
        reverse(str.begin(),str.end());
        
        //for(int i=0;i<str.size();i++){
        //    cout<<str[i];
        //}
        
        string ans="";
        
        for(i=0;i<str.size();i++){  //For adding space in str
            ans+=str[i];
            if(i!=str.size()-1) ans+=" ";
        }
          return ans;  
    }
};
